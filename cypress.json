{
  "baseUrl": "https://1wzho.top/bets/home",
  "chromeWebSecurity": true,
  "numTestsKeptInMemory": 1,
  "video": false ,
  "execTimeout": 180000,
  "viewportWidth": 1280,
  "viewportHeight": 800,
  "projectId": "kb155o",
  "env": {
    "cypress-plugin-snapshots": {
      "diffLines": 3,
      "excludeFields": [],
      "ignoreExtraFields": false,
      "normalizeJson": true,
      "prettier": true,
      "imageConfig": {
        "createDiffImage": true,
        "resizeDevicePixelRatio": true,
        "threshold": 0.5,
        "thresholdType": "percent"
      }
    }
  },
  "reporter": "mochawesome",
  "reporterOptions": {
    "reportDir": "cypress/allure-reports",
    "overwrite": false,
    "html": false,
    "json": true
  },
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ]
}
